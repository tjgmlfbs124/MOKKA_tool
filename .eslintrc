{
	"extends": ["react-app"],
	"parserOptions": {
		"ecmaVersion": 8,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"browser": true
	},
	"rules": {
		"no-const-assign": "warn",
		"no-new-object": "warn",
		"object-shorthand": "warn",
		"no-prototype-builtins": "warn",
		"no-array-constructor": "warn",
		"prefer-template": "warn",
		"no-useless-escape": "warn",
		"wrap-iife": [
			"warn",
			"inside"
		],
		"no-loop-func": "warn",
		"no-new-func": "warn",
		"space-before-function-paren": [
			"warn",
			"never"
		],
		"no-param-reassign": [
			"warn",
			{
				"props": false
			}
		],
		"prefer-spread": "warn",
		"arrow-spacing": "warn",
		"arrow-parens": [
			"warn",
			"always"
		],
		"no-confusing-arrow": [
			"warn",
			{
				"allowParens": true
			}
		],
		"no-useless-constructor": "warn",
		"no-dupe-class-members": "warn",
		"no-duplicate-imports": "warn",
		"dot-notation": "warn",
		"no-undef": [
			"warn",
			{
				"typeof": true
			}
		],
		"one-var": [
			"warn",
			"never"
		],
		"no-multi-assign": "warn",
		"no-case-declarations": "warn",
		"no-nested-ternary": "warn",
		"no-unneeded-ternary": "warn",
		"brace-style": "warn",
		"space-before-blocks": [
			"warn",
			{
				"functions": "always",
				"keywords": "always",
				"classes": "always"
			}
		],
		"space-infix-ops": "warn",
		"eol-last": [
			"warn",
			"always"
		],
		"newline-per-chained-call": "warn",
		"no-whitespace-before-property": "warn",
		"padded-blocks": [
			"warn",
			"never"
		],
		"space-in-parens": [
			"warn",
			"never"
		],
		"array-bracket-spacing": [
			"warn",
			"never"
		],
		"object-curly-spacing": [
			"warn",
			"always"
		],
		"max-len": [
			"warn",
			{
				"code": 120
			}
		],
		"comma-style": [
			"warn",
			"last"
		],
		"comma-dangle": [
			"warn",
			"always-multiline"
		],
		"semi": [
			"warn",
			"always"
		],
		"radix": "warn",
		"id-length": ["warn", {"exceptions": ["x", "y", "e"]}],
		"camelcase": [
			"warn",
			{
				"properties": "never"
			}
		],
		"new-cap": "warn",
		"curly": "warn",
		"keyword-spacing": "warn",
		"eqeqeq": "warn",
		"array-bracket-newline": [
			"warn",
			{
				"multiline": true,
				"minItems": 4
			}
		],
		"array-element-newline": [
			"warn",
			{
				"multiline": true,
				"minItems": 4
			}
		],
		"no-var": "error",
		"no-unused-vars": ["warn", {"caughtErrors": "none"}],
		"prefer-const": "warn",
		"indent": [
			"warn",
			4,
			{
				"SwitchCase": 1
			}
		],
		"linebreak-style": 0
	}
}
